        "bins = [0, 4515, 14005, 10**9]\n",
        "labels = [\"Low Income\", \"Moderate Income\", \"High Income\"]\n",
        "df1[\"EstimatedSalaryGroup\"] = pd.cut(\n",
        "    df1[\"EstimatedSalary\"],\n",
        "    bins=bins,\n",
        "    labels=labels,\n",
        "    right=True,\n",
        "    include_lowest=True\n",
        ")"
      ]
